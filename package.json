{
  "name": "contract-processor-backend",
  "version": "1.0.0",
  "description": "Backend para procesamiento de contratos con OpenAI Agents",
  "main": "dist/index.js",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc",
    "build:force": "tsc --noEmitOnError false",
    "start": "node dist/index.js",
    "test": "jest",
    "test:coverage": "jest --coverage --collectCoverageFrom='src/**/*.ts' --collectCoverageFrom='!src/**/*.d.ts'",
    "test:watch": "jest --watch",
    "db:migrate": "prisma migrate dev",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:studio": "prisma studio"
  },
  "keywords": [
    "openai",
    "agents",
    "contracts",
    "legal",
    "backend"
  ],
  "author": "Contract Processor Team",
  "license": "MIT",
  "dependencies": {
    "@openai/agents": "^0.1.0",
    "@openai/guardrails": "^0.1.0",
    "@prisma/client": "^5.7.1",
    "@types/multer": "^2.0.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "helmet": "^7.1.0",
    "multer": "^2.0.2",
    "openai": "^6.9.0",
    "prisma": "^5.7.1",
    "winston": "^3.11.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/helmet": "^4.0.0",
    "@types/jest": "^30.0.0",
    "@types/node": "^20.10.5",
    "@types/supertest": "^6.0.3",
    "jest": "^30.2.0",
    "supertest": "^7.1.4",
    "ts-jest": "^29.4.5",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
